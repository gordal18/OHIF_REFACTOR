@import "{ohif:design}/app"

ul
  width:92px
  z-index:1
  display:none
  padding-left:15px
  list-style-type:none
  .context-menu-item
    height:28px
    margin-bottom:5px
    span
      color:#91b9cd
      padding-left:40px
      margin-top:10px
  li
    background-repeat:no-repeat;
  #context_wl
    background: url("/images/nav-levels-off.svg");
    background-repeat:no-repeat;
  #context_zoom
    background: url("/images/nav-zoom-off.svg");
    background-repeat:no-repeat;
  #context_pan
    background: url("/images/nav-pan-off.svg");
    background-repeat:no-repeat;
  #context_reset
    background: url("/images/nav-reset-off.svg");
    background-repeat:no-repeat;
  #context_clear
    background: url("/images/nav-clear-off.svg");
    background-repeat:no-repeat;
